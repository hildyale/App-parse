<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="android:Theme.Material">
    </style>
    <color name="teal">#e0f2f1</color>
    <color name="teal2">#26a69a</color>
    <style name="teal" parent="android:Theme.Material.Light.DarkActionBar">
        <item name="android:colorPrimary">#26a69a</item>
        <item name="android:colorPrimaryDark">#004d40</item>
        <item name="android:colorAccent">#4db6ac</item>
        <item name="android:colorButtonNormal">#80cbc4</item>
        <item name="android:textColorPrimary">#000000</item>
        <item name="android:textColorSecondary">#004d40</item>
