    <color name="cyan">#e0f7fa</color>
    <style name="cyan" parent="android:Theme.Material.Light.DarkActionBar">
        <item name="android:colorPrimary">#26c6da</item>
        <item name="android:colorPrimaryDark">#006064</item>
        <item name="android:colorAccent">#80deea</item>
